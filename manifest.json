{
	"name": "InteractiveAPITool",
	"version": "0.2.0",
	"manifest_version": 2,
	"description": "Intacct API Builder and Poster",
	"app": {
		"launch":	{
			"local_path": "interactiveAPITool.html"
		}
	},
	"permissions": [ 
			"https://*.intacct.com/" 
	],
	"icons": 	{
		"128": "icon.png"
	},
	"content_security_policy": "script-src 'self'; object-src 'self'"
}